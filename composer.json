{
    "name": "drupal/controller_annotations",
    "type": "drupal-module",
    "description": "Integrates the controller annotations from Symfony Framework SensioFrameworkExtraBundle into Drupal",
    "keywords": [
        "annotation",
        "controller",
        "routing",
        "converter",
        "param",
        "template",
        "symfony",
        "framework"
    ],
    "homepage": "https://mediamonks.com/",
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "Robert Slootjes",
            "email": "robert@mediamonks.com",
            "homepage": "https://github.com/slootjes"
        },
        {
            "name": "Fabien Potencier",
            "email": "fabien@symfony.com",
            "homepage": "https://sensiolabs.com/"
        }
    ],
    "autoload": {
        "psr-4": {
            "Drupal\\controller_annotations\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Drupal\\Tests\\controller_annotations\\": "tests/src"
        }
    },
    "require": {
        "php": "^7.1",
        "symfony/config": "^3.0",
        "drupal/core": "^8.5"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.11|^1.0",
        "symfony/expression-language": "^3.0"
    },
    "suggest": {
        "symfony/expression-language": "Add support for cache annotations based on request variables"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
